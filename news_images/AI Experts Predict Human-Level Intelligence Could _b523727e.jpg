          if (!deviceId) {
            deviceId = generateDeviceId();
            localStorage.setItem("pymnts_device_id", deviceId);
            // TODO: add pymnts_device_id cookie
          }
          return deviceId;
        }

        const recordEvents = async (events) => {
          const data = {
            user_id: getDeviceId(),
            events: events
          };

          const reqURL = `/wp-json/pymnts/info?data=${JSON.stringify(data)}`;
